#explorer
  %h1= "API #{@version} Explorer"

  %h2
    Console
  #explorer_input
    .method
      %ul.dropdown
    .prefix= " http://root.api/path/#{@version}/"
    %input#path_input.path_input
    %button.try_button#try_button View

  #explorer_output
    %nav#explorer_nav
      %ul
        %li.tab.formatted_tab= link_to "Formatted JSON", "#"
        %li.tab.table_tab= link_to "Table Format", "#"
        %li.tab.raw_tab= link_to "Raw JSON", "#"
    
    #response